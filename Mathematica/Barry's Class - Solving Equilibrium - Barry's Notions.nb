 CellChangeTimes->{{3.663529976342022*^9, 3.663530003492758*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"findSymbols", "[", "expr_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "reaped", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"reaped", " ", "=", " ", 
      RowBox[{
       RowBox[{"Reap", "[", 
        RowBox[{"Scan", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"MatchQ", "[", 
              RowBox[{"#", ",", " ", "_Symbol"}], "]"}], ",", " ", 
             RowBox[{"Sow", "[", "#", "]"}]}], "]"}], "&"}], ",", " ", "expr",
           ",", " ", "Infinity"}], "]"}], "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", "reaped", "]"}], "\[Equal]", "0"}], ",", " ", 
        "reaped", ",", " ", 
        RowBox[{"reaped", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "]"}], " ", "//", " ", 
      "Union"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.663529684104897*^9, 3.663529705943718*^9}, {
